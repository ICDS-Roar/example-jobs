#!/bin/bash 
#PBS -l nodes=1:ppn=1 
#PBS -l walltime=10:00 
#PBS -l pmem=10gb 
#PBS -A open 
#PBS -o R-normalize.out
#PBS -e R-normalize.err

# Get started
echo " " 
echo "Job started on $(hostname) at $(date)" 
echo " "

# Load R
module purge
module use /gpfs/group/RISE/sw7/modules 
module load r/4.1.0

# Go to the correct place 
cd /gpfs/scratch/<user_id>/roar-example-jobs/R

# Run the job itself 
Rscript normalize.R

# Finish up 
echo " " 
echo "Job Ended at $(date)" 
echo " "
